{
  "name": "node-op",
  "version": "0.0.2",
  "op_version": "0.5.5",
  "description": "One password installer",
  "main": "index.js",
  "files": [
    "lib/op",
    "index.js",
    "install-op.js",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "node index.js",
    "prepublishOnly": "rimraf ./lib && mkdir ./lib && cp op ./lib/op",
    "postinstall": "node install-op.js",
    "obtain-fingerprint": "echo -n | openssl s_client -connect cache.agilebits.com:443 | sed -ne '/-BEGIN CERTIFICATE-/,/-END CERTIFICATE-/p' > cert.pem && openssl x509 -noout -in cert.pem -fingerprint"
  },
  "bin": {
    "op": "./lib/op"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaripych/node-op.git"
  },
  "keywords": [
    "one-password"
  ],
  "author": "@zaripych",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zaripych/node-op/issues"
  },
  "homepage": "https://github.com/zaripych/node-op#readme",
  "dependencies": {
    "adm-zip": "0.4.13"
  },
  "devDependencies": {
    "rimraf": "^2.6.2"
  }
}
